@model LoginUser

<h1>Login:</h1>

<a asp-action="Index" asp-controller="Home">Don't have an account</a>
<form class="col-5" asp-action="Signin" asp-controller="Home" method="post"><br>
<div class="form-group">
<span asp-validation-for="LoginEmail"></span>
<label asp-for="LoginEmail"></label>
<input asp-for="LoginEmail" class="form-control">
</div><br>
<div class="form-group">
<span asp-validation-for="LoginPassword"></span>
<label asp-for="LoginPassword"></label>
<input asp-for="LoginPassword" class="form-control">
</div>
<div class="form-group"><br>
       
<input type="Submit" value="SignIn" class="btn btn-outline-success">
</form>
</div>

